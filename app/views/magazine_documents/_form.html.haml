= form_for @magazine_document, :html => { :class => 'form-horizontal' } do |f|
  .control-group
    = f.label :supplier_id, :class => 'control-label'
    .controls
      = f.collection_select :supplier_id, Supplier.all, :id, :description, :class => 'text_field'
      = link_to t('new_supplier'), new_supplier_path, class: 'btn'
  .control-group
    = f.label :commodity_id, :class => 'control-label'
    .controls
      = f.collection_select :commodity_id, Commodity.all, :id, :name, :class => 'text_field'
  .control-group
    = f.label :quanity_gross, :class => 'control-label'
    .controls
      = f.text_field :quanity_gross, :class => 'text_field'
  .control-group
    = f.label :quanity_tare, :class => 'control-label'
    .controls
      = f.text_field :quanity_tare, :class => 'text_field'
  .control-group
    = f.label :quanity_net, :class => 'control-label'
    .controls
      = f.text_field :quanity_net, :class => 'text_field'
  .control-group
    = f.label :date, :class => 'control-label'
    .controls
      = f.date_select :date, use_month_names: t('months'), :class => 'text_field'
  .control-group
    = f.label :current_number, :class => 'control-label'
    .controls
      = f.text_field :current_number, value: current_number, :class => 'text_field'
  .control-group
    = f.label :humidity, :class => 'control-label'
    .controls
      = f.text_field :humidity, :class => 'text_field'
  .control-group
    = f.label :contamination, :class => 'control-label'
    .controls
      = f.text_field :contamination, :class => 'text_field'
  .control-group
    = f.label :document_type, :class => 'control-label'
    .controls
      = f.select :document_type, MagazineDocument::TYPES, :class => 'text_field'
  .control-group
    = f.label :vehicle_number, :class => 'control-label'
    .controls
      = f.text_field :vehicle_number, :class => 'text_field'
  .form-actions
    = f.submit t('activerecord.attributes.magazine_document.create'), :class => 'btn btn-primary'
    = link_to t('.cancel', :default => t("helpers.links.cancel")), magazine_documents_path, :class => 'btn'
